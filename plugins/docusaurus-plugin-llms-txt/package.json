  "devDependencies": {
    "@docusaurus/tsconfig": "^3",
    "@docusaurus/types": "^3",
    "@docusaurus/logger": "^3",
    "@docusaurus/utils": "^3",
    "@docusaurus/utils-validation": "^3",
    "@types/node": "^22.15.19",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@types/hast": "^3.0.4",
    "@types/fs-extra": "^11.0.4"
  },
  "peerDependencies": {
    "@docusaurus/core": "^3",
    "@docusaurus/logger": "^3",
    "@docusaurus/utils": "^3",
    "@docusaurus/utils-validation": "^3",
    "@types/hast": "^2 || ^3",
